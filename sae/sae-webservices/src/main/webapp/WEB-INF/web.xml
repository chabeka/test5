<?xml version="1.0" encoding="UTF-8"?>
<web-app
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   id="WebApp_ID"
   version="2.5">

   <display-name>sae-webservices</display-name>

   <welcome-file-list>
      <welcome-file>welcome.html</welcome-file>
   </welcome-file-list>

   <servlet>
      <servlet-name>AxisServlet</servlet-name>
      <servlet-class>org.apache.axis2.transport.http.AxisServlet</servlet-class>
      <load-on-startup>1</load-on-startup>
   </servlet>
   
   <servlet-mapping>
      <servlet-name>AxisServlet</servlet-name>
      <url-pattern>/services/*</url-pattern>
   </servlet-mapping>
   
   <servlet>
      <display-name>EtatSaeServlet</display-name>
      <servlet-name>EtatSaeServlet</servlet-name>
      <servlet-class>fr.urssaf.image.sae.exploitation.servlet.EtatSaeServlet</servlet-class>
   </servlet>

   <servlet-mapping>
      <servlet-name>EtatSaeServlet</servlet-name>
      <url-pattern>/SAETest</url-pattern>
   </servlet-mapping>
  
  <servlet>
    <display-name>EtatSaeFastServlet</display-name>
    <servlet-name>EtatSaeFastServlet</servlet-name>
    <servlet-class>fr.urssaf.image.sae.exploitation.servlet.EtatSaeFastServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>EtatSaeFastServlet</servlet-name>
    <url-pattern>/SAEFastTest</url-pattern>
  </servlet-mapping>
  
</web-app>