<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<keyspace xmlns="http://xml.dataset.cassandraunit.org"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://xml.dataset.cassandraunit.org cassandraUnitDataset.xsd ">
   <name>Batch</name>
   <strategy>org.apache.cassandra.locator.SimpleStrategy</strategy>
   <replicationFactor>1</replicationFactor>
   <columnFamilies>
      <columnFamily>
         <name>JobInstance</name>
         <comparatorType>BytesType</comparatorType>
         <columnMetadata>
            <name>jobKey</name>
            <validationClass>BytesType</validationClass>
            <indexType>KEYS</indexType>
         </columnMetadata>
      </columnFamily>
      <columnFamily>
         <name>JobInstancesByName</name>
         <comparatorType>LongType</comparatorType>
      </columnFamily>
      <columnFamily>
         <name>JobInstanceToJobExecution</name>
         <comparatorType>LongType</comparatorType>
      </columnFamily>
      <columnFamily>
         <name>JobExecution</name>
         <comparatorType>BytesType</comparatorType>
      </columnFamily>
      <columnFamily>
         <name>JobExecutions</name>
         <comparatorType>LongType</comparatorType>
      </columnFamily>
      <columnFamily>
         <name>JobExecutionsRunning</name>
         <comparatorType>LongType</comparatorType>
      </columnFamily>
      <columnFamily>
         <name>JobExecutionToJobStep</name>
         <comparatorType>LongType</comparatorType>
      </columnFamily>
      <columnFamily>
         <name>JobStep</name>
         <comparatorType>BytesType</comparatorType>
      </columnFamily>
      <columnFamily>
         <name>JobSteps</name>
         <comparatorType>LongType</comparatorType>
      </columnFamily>
      <columnFamily>
         <name>Sequences</name>
         <comparatorType>BytesType</comparatorType>
      </columnFamily>
   </columnFamilies>
</keyspace>